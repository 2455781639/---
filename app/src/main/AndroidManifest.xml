<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.kirkhorn.knut.android_sudoku" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tubiao"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".rank_listActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="me.kirkhorn.knut.rank_listActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:configChanges="locale"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameActivity"
            android:exported="false" >
            <intent-filter>
                <action android:name="me.kirkhorn.knut.GameActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewBoardActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="me.kirkhorn.knut.NewBoardActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InstructionsActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="me.kirkhorn.knut.InstructionsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseNumberActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="me.kirkhorn.knut.ChooseNumberActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameDifficultyActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="me.kirkhorn.knut.GameDifficultyActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>